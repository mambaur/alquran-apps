<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Drop Shadow Stack -->

    <item>
        <shape>
            <padding android:right="1dp" android:bottom="1dp"/>
            <solid android:color="#14EAE7E7" />
            <corners android:radius="22dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:right="1dp" android:bottom="1dp" android:left="1dp" />
            <solid android:color="#204E4D4D" />
            <corners android:radius="19dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:top="1dp" android:right="1dp" android:bottom="1dp" android:left="1dp" />
            <solid android:color="#25434343" />
            <corners android:radius="17dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:top="1dp" android:right="1dp" android:bottom="1dp" android:left="1dp" />
            <solid android:color="#30424141" />
            <corners android:radius="15dp" />
        </shape>
    </item>

    <!-- Background -->
    <item>
        <shape>
            <solid android:color="@color/colorWhite" />
            <corners android:radius="15dp" />
        </shape>
    </item>
</layer-list>